<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:bind="http://schemas.android.com/tools"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <import type="com.vastu.realestate.appModule.dashboard.bindingAdapter.FiletrBindingAdapter"/>
        <variable
            name="realEstateViewModel"
            type="com.vastu.realestate.appModule.dashboard.viewmodel.RealEstateViewModel" />
    </data>
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/pinkk">

    <LinearLayout
        android:id="@+id/llTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_margin="@dimen/dimen10dp"
       >
        <TextView
            android:id="@+id/txt_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@{realEstateViewModel.title}"
            android:textSize="@dimen/text_size_small"
            app:fontFamily="@font/open_sans_bold"
            android:textColor="@color/filter_txt_color"
            />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/llCityLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/llTitle"
        setVisibility="@{realEstateViewModel.isVisibleCityLayout}"
        android:layout_margin="@dimen/dimen10dp"

        >
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/tilcity"
            style="@style/Widget.Design.TextInputLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:textAlignment="center"
            android:textColorHint="@color/black"
            app:boxStrokeColor="@color/black"
            app:endIconDrawable="@drawable/drop_down_icon"
            app:endIconMode="custom"
            app:expandedHintEnabled="false"
            app:helperText="@string/required"
            app:helperTextTextColor="@color/red"
            app:hintEnabled="false"
            app:placeholderText="@string/city_text"
            app:placeholderTextColor="@color/black"
          >

            <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                android:id="@+id/autoCompleteCity"
                style="@style/TextAppearance"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@drawable/edittext_background"
                android:clickable="true"
                android:context="@{context}"
                android:dropDownHeight="136dp"
                android:editable="false"
                android:focusable="false"
                android:focusableInTouchMode="true"
                android:gravity="center_vertical"
                android:onItemClick="@{realEstateViewModel}"
                tilLayout="@{tilcity}"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_small"
                tools:ignore="SpeakableTextPresentCheck"
                android:padding="@dimen/dimen10dp" />

        </com.google.android.material.textfield.TextInputLayout>

    </LinearLayout>
    <LinearLayout
        android:id="@+id/llSubAreaLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/llCityLayout"
        setVisibility="@{realEstateViewModel.isVisibleSubAreaLayout}"
        >

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rlAreaListRecyclerView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:listitem="@layout/sub_area_list_adapter" />


    </LinearLayout>
    <LinearLayout
        android:id="@+id/llBudgetLayout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_margin="@dimen/dimen10dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/llSubAreaLayout"
        setVisibility="@{realEstateViewModel.isVisibleBudgetLayout}"
      >

        <ImageView
            android:id="@+id/imgRange"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:src="@{realEstateViewModel.range}" />
<LinearLayout
    android:id="@+id/llLimitBar"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:layout_below="@id/imgRange"
    android:weightSum="1"
    >

        <TextView
            android:id="@+id/txtLimitStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_regular"
            android:gravity="start"
            android:text="@{realEstateViewModel.lowerLimit}"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_size_small"
            android:layout_weight="0.5"
            />

        <TextView
            android:id="@+id/txtLimitEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_regular"
            android:gravity="end"
            android:text="@{realEstateViewModel.upperLimit}"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_size_small"
            android:layout_weight="0.5"
            />
</LinearLayout>
        <com.google.android.material.slider.RangeSlider
            android:id="@+id/budgetRangeSlider"
            viewmodel="@{realEstateViewModel}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/llLimitBar"
            lowerLimit="@{realEstateViewModel.lowerLimit}"
            upperLimit="@{realEstateViewModel.upperLimit}"
            updatedValue="@{realEstateViewModel}"
            android:valueFrom="@string/budget_lower_limit"
            android:valueTo="@string/budget_upper_limit"
            app:haloColor="@color/white"
            app:labelBehavior="gone"

            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/llProprtyType"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="top"
        app:layout_constraintTop_toBottomOf="@+id/llBudgetLayout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_margin="@dimen/dimen10dp"
        setVisibility="@{realEstateViewModel.isVisiblePropertyLayout}"

        >
        <androidx.appcompat.widget.AppCompatCheckBox
            android:id="@+id/checkHouses"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/houses_and_Villas"
            android:textAppearance="@style/TextAppearance"
            android:textColor="@color/black"
            isChecked="@{realEstateViewModel}"
            android:checked="@{realEstateViewModel.isHousesSelected}"/>
        <androidx.appcompat.widget.AppCompatCheckBox
            android:id="@+id/checkApartment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/apartments"
            android:textAppearance="@style/TextAppearance"
            android:textColor="@color/black"
            isChecked="@{realEstateViewModel}"
            android:checked="@{realEstateViewModel.isApartmentSelected}"/>
        <androidx.appcompat.widget.AppCompatCheckBox
            android:id="@+id/checkBuilderFloor"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/builder_floors"
            android:textAppearance="@style/TextAppearance"
            android:textColor="@color/black"
            isChecked="@{realEstateViewModel}"
            android:checked="@{realEstateViewModel.isBuilerFloorSelected}"/>
        <androidx.appcompat.widget.AppCompatCheckBox
            android:id="@+id/check_farm_house"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/farmhouses"
            isChecked="@{realEstateViewModel}"
            android:checked="@{realEstateViewModel.isFarmHousesSelected}"
            android:textAppearance="@style/TextAppearance"
            android:textColor="@color/black"
            />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/llPricePerSqFtLayout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_margin="@dimen/dimen10dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/llProprtyType"
        setVisibility="@{realEstateViewModel.isVisiblePricePerSqFtLayout}"
        >

        <ImageView
            android:id="@+id/imgRangeForSqFt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:src="@drawable/budget_range_icon" />
        <LinearLayout
            android:id="@+id/llSqFtLimitBar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@id/imgRangeForSqFt"
            android:weightSum="1"
            >

            <TextView
                android:id="@+id/txtLowerLimit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_regular"
                android:gravity="start"
                android:text="@{realEstateViewModel.lowerLimitForPerSq}"
                android:textColor="@color/text_color"
                android:textSize="@dimen/text_size_small"
                android:layout_weight="0.5"/>

            <TextView
                android:id="@+id/txtUpperLimit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_regular"
                android:gravity="end"
                android:text="@{realEstateViewModel.upperLimitForPerSq}"
                android:textColor="@color/text_color"
                android:textSize="@dimen/text_size_small"
                android:layout_weight="0.5"/>
        </LinearLayout>
        <com.google.android.material.slider.RangeSlider
            android:id="@+id/rangeSliderForSqFt"
            viewmodel="@{realEstateViewModel}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/llSqFtLimitBar"
            android:value="@string/SqFt_upper_limit"
            android:valueFrom="@string/budget_lower_limit"
            android:valueTo="@string/SqFt_upper_limit"
            app:haloColor="@color/white"
            app:labelBehavior="gone"
            app:values="@array/rsPerSqSliderValues" />


    </LinearLayout>
    <LinearLayout
        android:id="@+id/llNoOfBedroom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dimen5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/llPricePerSqFtLayout"
        setVisibility="@{realEstateViewModel.isVisibleByBedroomsLayout}"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/txtBedroomOne"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/onebedroom"
            style="@style/TextAppearance"
            android:textColor="@color/black"
            android:padding="@dimen/dimen10dp"
            android:layout_margin="@dimen/dimen5dp"
            android:background="@drawable/filter_option_background_on_disselect"
            viewmodel="@{realEstateViewModel}"
            />
        <TextView
            android:id="@+id/txtBedroomTwo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/dimen5dp"
            android:background="@drawable/filter_option_background_on_disselect"
            android:padding="@dimen/dimen10dp"
            android:text="@string/twobedrooms"
            style="@style/TextAppearance"
            viewmodel="@{realEstateViewModel}"
            />
        <TextView
        android:id="@+id/txtBedroomThree"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/threebedrooms"
        android:padding="@dimen/dimen10dp"
        android:layout_margin="@dimen/dimen5dp"
        style="@style/TextAppearance"
        android:background="@drawable/filter_option_background_on_disselect"
        viewmodel="@{realEstateViewModel}"

            />
        <TextView
        android:id="@+id/txtBedroomFour"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/fourbedrooms"
        android:padding="@dimen/dimen10dp"
        android:layout_margin="@dimen/dimen5dp"
        style="@style/TextAppearance"
        android:background="@drawable/filter_option_background_on_disselect"
        viewmodel="@{realEstateViewModel}"
            />


    </LinearLayout>
    <LinearLayout
        android:id="@+id/llNoOfBathroom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dimen5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/llNoOfBedroom"
        setVisibility="@{realEstateViewModel.isVisibleByBathroomsLayout}"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/txtBathroomOne"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/onebathroom"
            android:padding="@dimen/dimen10dp"
            android:layout_margin="@dimen/dimen5dp"
            style="@style/TextAppearance"
            android:background="@drawable/filter_option_background_on_disselect"
            viewmodel="@{realEstateViewModel}"
            />
        <TextView
            android:id="@+id/txtBathroomTwo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/twobathrooms"
            android:padding="@dimen/dimen10dp"
            android:layout_margin="@dimen/dimen5dp"
            style="@style/TextAppearance"
            android:background="@drawable/filter_option_background_on_disselect"
            viewmodel="@{realEstateViewModel}"
            />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/llFurnishingType"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="top"
        app:layout_constraintTop_toBottomOf="@+id/llNoOfBathroom"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_margin="@dimen/dimen10dp"
        setVisibility="@{realEstateViewModel.isVisibleByFurnishingLayout}"

        >
        <androidx.appcompat.widget.AppCompatCheckBox
            android:id="@+id/checkFurnished"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/furnished"
            android:textAppearance="@style/TextAppearance"
            android:textColor="@color/black"
            isChecked="@{realEstateViewModel}"
            android:checked="@{realEstateViewModel.isFurnished}"/>
        <androidx.appcompat.widget.AppCompatCheckBox
            android:id="@+id/checkUnfurnished"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/unfurnished"
            android:textAppearance="@style/TextAppearance"
            android:textColor="@color/black"
            isChecked="@{realEstateViewModel}"
            android:checked="@{realEstateViewModel.isUnfurnished}"/>
        <androidx.appcompat.widget.AppCompatCheckBox
            android:id="@+id/checkSemiFurnished"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/semi_furnished"
            android:textAppearance="@style/TextAppearance"
            android:textColor="@color/black"
            isChecked="@{realEstateViewModel}"
            android:checked="@{realEstateViewModel.isSemiFurnished}"/>


    </LinearLayout>
    <LinearLayout
        android:id="@+id/llConstructionStatus"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="top"
        app:layout_constraintTop_toBottomOf="@+id/llFurnishingType"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_margin="@dimen/dimen10dp"
        setVisibility="@{realEstateViewModel.isVisibleConstructionStsLayout()}"

        >
        <androidx.appcompat.widget.AppCompatCheckBox
            android:id="@+id/checkUnderConst"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/under_const"
            android:textAppearance="@style/TextAppearance"
            android:textColor="@color/black"
            isChecked="@{realEstateViewModel}"
            android:checked="@{realEstateViewModel.isUnderConst}"/>
        <androidx.appcompat.widget.AppCompatCheckBox
            android:id="@+id/checkReadyToMove"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/ready_to_move"
            android:textAppearance="@style/TextAppearance"
            android:textColor="@color/black"
            isChecked="@{realEstateViewModel}"
            android:checked="@{realEstateViewModel.isReadyToMove}"/>
        <androidx.appcompat.widget.AppCompatCheckBox
            android:id="@+id/checkNewLaunch"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/new_launch"
            android:textAppearance="@style/TextAppearance"
            android:textColor="@color/black"
            isChecked="@{realEstateViewModel}"
            android:checked="@{realEstateViewModel.isNewLaunch}"/>


    </LinearLayout>
    <LinearLayout
        android:id="@+id/llListedBy"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="top"
        app:layout_constraintTop_toBottomOf="@+id/llConstructionStatus"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_margin="@dimen/dimen10dp"
        setVisibility="@{realEstateViewModel.isVisibleListedLayout()}"

        >
        <androidx.appcompat.widget.AppCompatCheckBox
            android:id="@+id/check_owner"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/owner"
            android:textAppearance="@style/TextAppearance"
            android:textColor="@color/black"
            isChecked="@{realEstateViewModel}"
            android:checked="@{realEstateViewModel.isOwner}"/>
        <androidx.appcompat.widget.AppCompatCheckBox
            android:id="@+id/check_dealer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/dealer"
            android:textAppearance="@style/TextAppearance"
            android:textColor="@color/black"
            isChecked="@{realEstateViewModel}"
            android:checked="@{realEstateViewModel.isDealer}"/>
        <androidx.appcompat.widget.AppCompatCheckBox
            android:id="@+id/check_builder"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/builder"
            android:textAppearance="@style/TextAppearance"
            android:textColor="@color/black"
            isChecked="@{realEstateViewModel}"
            android:checked="@{realEstateViewModel.isBuilder}"/>


    </LinearLayout>
    <LinearLayout
        android:id="@+id/llBuildupAreaLayout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_margin="@dimen/dimen10dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/llListedBy"
        setVisibility="@{realEstateViewModel.isVisibleBuildupAreaLayout()}"
        >
        <LinearLayout
            android:id="@+id/llLimitValue"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="1"
            >

            <TextView
                android:id="@+id/txtLowerLimitBuildupAr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_regular"
                android:gravity="start"
                android:text="@{realEstateViewModel.lowerLimitForBuildupArea}"
                android:textColor="@color/icon_color"
                android:textSize="@dimen/text_size_small"
                android:layout_weight="0.5"/>

            <TextView
                android:id="@+id/txtUpperLimitBuildupAr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_regular"
                android:gravity="end"
                android:text="@{realEstateViewModel.upperLimitForBuildupArea}"
                android:textColor="@color/text_color"
                android:textSize="@dimen/text_size_small"
                android:layout_weight="0.5"/>
        </LinearLayout>
        <com.google.android.material.slider.RangeSlider
            android:id="@+id/rangeSliderForBuildupAr"
            viewmodel="@{realEstateViewModel}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/llLimitValue"
            lowerLimit="@{realEstateViewModel.lowerLimitForBuildupArea}"
            upperLimit="@{realEstateViewModel.upperLimitForBuildupArea}"
            updatedValue="@{realEstateViewModel}"
            android:value="@string/buildup_area_upper_limit"
            android:valueFrom="@string/budget_lower_limit"
            android:valueTo="@string/buildup_area_upper_limit"
            app:haloColor="@color/white"
            app:labelBehavior="gone"
             />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/llChangeSort"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dimen5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/llBuildupAreaLayout"
        setVisibility="@{realEstateViewModel.isVisibleChangeSortLayout}"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/txtDatePublished"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/date_published"
            style="@style/TextAppearance"
            android:padding="@dimen/dimen10dp"
            android:layout_margin="@dimen/dimen5dp"
            android:background="@drawable/filter_option_background_on_disselect"
            viewmodel="@{realEstateViewModel}"
            />
        <TextView
            android:id="@+id/txtRelevance"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/relevance"
            style="@style/TextAppearance"
            android:padding="@dimen/dimen10dp"
            android:layout_margin="@dimen/dimen5dp"
            android:background="@drawable/filter_option_background_on_disselect"
            viewmodel="@{realEstateViewModel}"
            />
        <TextView
            android:id="@+id/txtPriceIncr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/price_low_to_high"
            style="@style/TextAppearance"
            android:padding="@dimen/dimen10dp"
            android:layout_margin="@dimen/dimen5dp"
            android:background="@drawable/filter_option_background_on_disselect"
            viewmodel="@{realEstateViewModel}"
            />
        <TextView
            android:id="@+id/txtPriceDecr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/price_high_to_low"
            style="@style/TextAppearance"
            android:padding="@dimen/dimen10dp"
            android:layout_margin="@dimen/dimen5dp"
            android:background="@drawable/filter_option_background_on_disselect"
            viewmodel="@{realEstateViewModel}"
            />
        <TextView
            android:id="@+id/txtDistance"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/distance"
            style="@style/TextAppearance"
            android:padding="@dimen/dimen10dp"
            android:layout_margin="@dimen/dimen5dp"
            android:background="@drawable/filter_option_background_on_disselect"
            viewmodel="@{realEstateViewModel}"
            />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
</layout>